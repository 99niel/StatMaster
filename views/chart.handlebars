<script id="tabular-template" type="text/html">
<h2>Tabular Data</h2>
<table class="table table-bordered">
  <thead>
    <tr>
      <td>Month</td>
      \{{#each categories}} <td>\{{label}}</td> \{{/each }}
    </tr>
  </thead>
  <tbody>
    <tr>
      <td>Shortlisted</td>
      \{{#each empShortlisted}} <td>\{{value}}</td> \{{/each }}
    </tr>

    <tr>
      <td>Selected</td>
      \{{#each empSelected}} <td>\{{value}}</td> \{{/each }}
    </tr>
  </tbody>
</table>
</script>